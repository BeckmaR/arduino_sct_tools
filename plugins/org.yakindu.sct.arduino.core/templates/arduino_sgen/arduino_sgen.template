GeneratorModel for yakindu::arduino_cpp {

	statechart ${statechartName} {

		feature LicenseHeader {
			licenseText = "Generated by YAKINDU Statechart Tools for Arduino v0.5.0"
		}

		feature Outlet {
			targetProject = "${projectName}"
			targetFolder = "${srcGenFolder}"
		}

		feature Arduino {
			userSrcFolder = "${srcFolder}"
			timer = "${timer}"
			cyclePeriod = "${cyclePeriod}"
		}
	}
}